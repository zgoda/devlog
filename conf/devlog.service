[Unit]
Description=uWSGI launcher for Devlog
After=network.target

[Service]
User=jarek
Group=www-data
WorkingDirectory=/home/jarek/devlog
Environment="PATH=/home/jarek/devlog/_venv/bin"
Environment="DEVLOG_CONFIG_LOCAL=/home/jarek/devlog/config/config_local.py"
Environment="DEVLOG_CONFIG_SECRETS=/home/jarek/devlog/config/secrets.py"
ExecStart=/home/jarek/devlog/_venv/bin/uwsgi --ini config/devlog.ini

[Install]
WantedBy=multi-user.target
